#!/usr/bin/env python3
import os
import sys
sys.path.insert(0, os.path.join(os.environ['CHARM_DIR'], 'lib'))
import hookenv

def config_changed():
    """
    We let the log know the message was changed.
    """
    hookenv.log(" ========= hook: config-changed  ========")


if __name__ == "__main__":
    config_changed()